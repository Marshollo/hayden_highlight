{
  "manifest_version": 3,
  "name": "Special Place Catcher",
  "version": "1.1",
  "description": "MTA, PPA, LA",
  "permissions": ["scripting", "activeTab"],
  "host_permissions": ["https://beta.console.hayden.ai/*"],
  "action": {
    "default_title": "Catcher"
  },
  "content_scripts": [
    {
      "matches": ["https://beta.console.hayden.ai/*"],
      "js": ["content.js"]
    }
  ]
}
